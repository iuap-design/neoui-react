    "scripts": {
        "start": "gulp pack_demo && cross-env NODE_ENV=development webpack serve --config webpack.dev.config.js",
        "build": "cross-env NODE_ENV=production PIPPELINE=pro webpack --config webpack.build.config.js",
        "build:daily": "npm run prebuild && cross-env NODE_ENV=production PIPPELINE=daily webpack --config webpack.build.config.js && npm run postbuild",
        "prebuild": "npm run build:types & gulp pack_demo",
        "postbuild": "gulp demo_api",
        "analyzer": "cross-env NODE_ENV=production webpack --config webpack.analyzer.config.js && gulp demo_api",
        "build:api": "gulp demo_api",
        "build:pack": "gulp pack_demo",
        "build:icon": "gulp create_icon_demo",
        "build:types": "tsc -p ./tsconfig.json --emitDeclarationOnly",
        "postinstall": "patch-package --patch-dir ./patches --patch-depth 2"
    },
